{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": false,
        "declaration": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "strictNullChecks": true,
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es2015",
        "allowJs": true,
        "importHelpers": true,
        "downlevelIteration": true,
        "types": [
            
            "webpack-env"
        ],
        "lib": [
            "DOM",
            "ES2015"
        ]
    },
    "include": ["src"]
}